{
    "headers": [
        {
            "source": "/(.*)\\.js$",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                },
                {
                    "key": "Content-Encoding",
                    "value": "br"
                }
            ]
        }
    ],
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "installCommand": "npm install --legacy-peer-deps"
            }
        }
    ]
}